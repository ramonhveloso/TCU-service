{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Uvicorn",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/uvicorn",
            "args": [
                "app.main:app",
                "--reload"
            ],
            "env": {
                "ENVIRONMENT": "env"
            },
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
